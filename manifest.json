  {
    "name": "Export 2019 Calcentral Calendar to ical",
    "version": "0.1",
    "description": "Downloads the enrolled courses schedule",
    "manifest_version": 2,
    "permissions": ["downloads","https://berkeley.collegescheduler.com/*","https://bcsweb.is.berkeley.edu/*"],
    "content_scripts": [{
        "matches": ["https://berkeley.collegescheduler.com/*","https://bcsweb.is.berkeley.edu/*"],
        "js": ["inject.js"],
        "run_at": "document_start"
    }],
    "web_accessible_resources": ["injected.js"],
    "browser_action": {
    "default_icon": "icon.png",
    "default_popup": "popup.html"
  }
  }
